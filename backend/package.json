{
  "name": "unimib-calendar-backend",
  "version": "1.0.0",
  "description": "unimib calendar backend",
  "contributors": [
    "Fabio Grieco f.grieco5@campus.unimib.it",
    "Omar Ghetti o.ghetti@campus.unimib.it"
  ],
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:ci": "jest --passWithNoTests --coverage",
    "eslint": "eslint \"**/*.js\"",
    "eslint:fix": "eslint --fix \"**/*.js\"",
    "eslint:ci": "eslint --fix \"**/*.js\" --max-warnings=0",
    "prettier:check": "prettier --check \"./routes/**/*.{js,json}\" \"./models/**/*.{js,json}\"",
    "prettier:write": "prettier --write \"./routes/**/*.{js,json}\" \"./models/**/*.{js,json}\"",
    "prettier:precommit": "pretty-quick --staged \"./routes/**/*.{js,json}\" \"./models/**/*.{js,json}\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.0.0",
    "mongoose": "^5.9.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-twitter": "^1.0.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
