{
  "name": "unimib-calendar-backend",
  "version": "1.0.0",
  "description": "unimib calendar backend",
  "contributors": [
    "Fabio Grieco f.grieco5@campus.unimib.it",
    "Omar Ghetti o.ghetti@campus.unimib.it"
  ],
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "./node_modules/eslint/bin/eslint.js --fix \"**/*.js\"",
    "prettier": "prettier --write \"**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js}": [
      "npm run prettier",
      "npm run lint"
    ]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.5"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
